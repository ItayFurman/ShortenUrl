
<form method="post">
    <div class="mb-3">
        <label for="url" class="form-label">Url to shorten:</label>
        <input id="url" type="url"  class="form-control" />
        <div id="validateUrl" class="text-danger"></div>
    </div>

    <div class="mb-3">
        <button id="btnSubmit" type="button" class="btn btn-sm btn-success">Shorten</button>
    </div>
</form>

<hr />
<p>
    Your short url is:
    <input id="shortUrl" readonly class="form-control" />
</p>

@section Scripts {
    <script src="~/js/JavaScript.moudle.js" asp-append-version="true" type="module"> </script>}